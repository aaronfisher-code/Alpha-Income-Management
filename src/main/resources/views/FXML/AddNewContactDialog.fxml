<?xml version="1.0" encoding="UTF-8"?>

<?import io.github.palexdev.materialfx.controls.MFXButton?>
<?import io.github.palexdev.materialfx.controls.MFXTextField?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.paint.Color?>

<StackPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AddNewContactDialogController">
   <children>
      <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="25.0" StackPane.alignment="CENTER">
         <children>
            <MFXTextField fx:id="newContactField" borderGap="0.0" floatMode="ABOVE" floatingText="Contact Name" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
               <textFill>
                  <Color />
               </textFill>
            </MFXTextField>
            <MFXTextField fx:id="accountCodeField" borderGap="0.0" floatMode="ABOVE" floatingText="Account Code" maxWidth="1.7976931348623157E308" text="200">
               <textFill>
                  <Color />
               </textFill>
            </MFXTextField>
            <HBox alignment="CENTER" spacing="30.0">
               <children>
                  <MFXButton minWidth="115.0" onAction="#addContact" style="-fx-background-color: #0F60FF;" text="Add new Contact" textFill="WHITE" HBox.hgrow="ALWAYS" />
                  <MFXButton minWidth="115.0" onAction="#closeDialog" style="-fx-border-color: #6e6b7b; -fx-border-radius: 5;" text="Cancel" textFill="#6e6b7b" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="40.0" />
         </padding>
      </VBox>
   </children>
   <stylesheets>
      <URL value="@../CSS/application.css" />
      <URL value="@../CSS/AddContactDialog.css" />
   </stylesheets>
</StackPane>
